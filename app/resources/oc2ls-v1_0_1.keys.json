        {
          "FieldID": 102,
          "String": "Processing",
          "Description": "an interim Response used to inform the Producer that the Consumer has accepted the Command but has not yet completed it."
        },
        {
          "FieldID": 200,
          "String": "OK",
          "Description": "the Command has succeeded."
        },
        {
          "FieldID": 400,
          "String": "BadRequest",
          "Description": "the Consumer cannot process the Command due to something that is perceived to be a Producer error (e.g., malformed Command syntax)."
        },
        {
          "FieldID": 401,
          "String": "Unauthorized",
          "Description": "the Command Message lacks valid authentication credentials for the target resource or authorization has been refused for the submitted credentials."
        },
        {
          "FieldID": 403,
          "String": "Forbidden",
          "Description": "the Consumer understood the Command but refuses to authorize it."
        },
        {
          "FieldID": 404,
          "String": "NotFound",
          "Description": "the Consumer has not found anything matching the Command."
        },
        {
          "FieldID": 500,
          "String": "InternalError",
          "Description": "the Consumer encountered an unexpected condition that prevented it from performing the Command."
        },
        {
          "FieldID": 501,
          "String": "NotImplemented",
          "Description": "the Consumer does not support the functionality required to perform the Command."
        },
        {
          "FieldID": 503,
          "String": "ServiceUnavailable",
          "Description": "the Consumer is currently unable to perform the Command due to a temporary overloading or maintenance of the Consumer."
