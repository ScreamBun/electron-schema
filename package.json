  "scripts": {
    "start": "cross-env NODE_ENV=production electron ./app/main.prod.js",
    "start-dev": "cross-env HOT=1 NODE_ENV=development electron -r @babel/register ./app/main.dev.js",
    "build": "concurrently \"npm run build-main\" \"npm run build-renderer\"",
    "build-main": "cross-env NODE_ENV=production webpack --config ./config/webpack.main.prod.js --colors",
    "build-renderer": "cross-env NODE_ENV=production webpack --config ./config/webpack.renderer.prod.js --colors",
    "package": "npm run build && electron-packager ./ --out=./builds --platform=all"
  },
  "dependencies": {
